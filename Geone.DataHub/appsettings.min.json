{
  "Server": {
    "VirtualPath": "datahub"
  },
  "Serilog": {
    "Properties": {
      "Application": "datahub"
    },
    "Using": [ "Serilog.Sinks.RollingFile" ],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Serilog": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "logs/log-{Date}.log"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  }
}
